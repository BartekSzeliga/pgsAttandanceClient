{
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": [ 1 ],
        "no-unreachable": [ 1 ],
        "no-undef": [ 1 ],
        "no-cond-assign": [ 1 ],
        "no-redeclare": [ 1 ],
        "no-fallthrough": [ 1 ],
        "no-console": [ 1 ],
        "no-inner-declarations": [ 1 ],
        "no-empty": [ 1 ],
        "comma-dangle": [ 1 ],
        "no-irregular-whitespace": [ 1 ],
        "no-extra-boolean-cast": [ 1 ],
        "no-ex-assign": [ 1 ],
        "no-func-assign": [ 1 ],
        "no-constant-condition": [ 1 ],
        "no-control-regex": [ 1 ],
        "no-extra-semi": [ 1 ],
        "no-mixed-spaces-and-tabs": [ 1 ],
        "no-sparse-arrays": [ 1 ],
        "undefined": [ 1 ]
    }
}
